@model RegTesting.Contracts.Domain.Error

@{
	ViewBag.Title = "Set TfsWorkItem - RegTesting";
}

<h2>Set TfsWorkItem</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("Edit","Error")) {
        @Html.ValidationSummary(true)
        <fieldset>
            <legend>Error</legend>
    
            @Html.HiddenFor(model => model.ID)
 
			<div class="display-label">Type</div>
			<div class="display-field">
				@Html.DisplayFor(model => model.Type)
			</div>
    
			<div class="display-label">Message</div>
			<div class="display-field">
				@Html.DisplayFor(model => model.Message)
			</div>
    
			<div class="display-label">StackTrace</div>
			<div class="display-field">
				@Html.DisplayFor(model => model.StackTrace)
			</div>
    
			<div class="display-label">InnerException</div>
			<div class="display-field">
				@Html.DisplayFor(model => model.InnerException)
			</div>
    
            <div class="editor-label">
                @Html.LabelFor(model => model.TfsWorkItemID)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.TfsWorkItemID)
                @Html.ValidationMessageFor(model => model.TfsWorkItemID)
            </div>
    
            <p>
                <input type="submit" value="Save" />
            </p>
        </fieldset>
    }
    
    <div>
        @Html.ActionLink("Back to List", "Index","Testing")
    </div>